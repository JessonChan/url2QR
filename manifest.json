{
  "manifest_version": 2,

  "name": "url2QR",
  "homepage_url":"http://www.meituan.com",
  "update_url": "http://clients2.google.com/service/update2/crx",
  "description": "Extension to make a QR image from selected tab's URL",
  "version": "1.1.0.2",
  "icons": 
  { 
    "16": "icon16.png",             
    "48": "icon48.png",            
    "128": "icon128.png" 
  },  
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [ "tabs"]
}
